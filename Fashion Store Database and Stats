CREATE TABLE Dresses (id INTEGER PRIMARY KEY, color TEXT, size_range TEXT, quantity INTEGER, price REAL);
INSERT INTO Dresses VALUES (1,"Blue","XS-XL", 10, 15.50);
INSERT INTO Dresses VALUES (2,"Black","XS-XL",12, 20.00);
INSERT INTO Dresses VALUES(3,"Red","XS-XL",15, 18.50);
SELECT * FROM Dresses WHERE price ORDER BY price;
SELECT * FROM Dresses WHERE "quanity" > 12;
